{
    "name": "jibaymcs/templatist",
    "description": "Create a blade component based on a template",
    "type": "library",
    "keywords": ["template", "blade", "component", "commands", "laravel"],
    "authors": [
        {
            "name": "JibayMcs",
            "email": "theamateis@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.3|^7.4|^8.0",
        "illuminate/support": "~6.0|~7.0|~8.0"
    },
    "autoload": {
        "psr-4": {
            "Templatist\\": "src/Templatist/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Templatist\\TemplatistServiceProvider"
            ]
        }
    }
}
